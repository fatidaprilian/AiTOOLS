{
  "version": 2,

  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "installCommand": "composer install --no-dev --prefer-dist",
  "buildCommand": "php artisan migrate --force && php artisan optimize:clear && php artisan config:cache && php artisan route:cache && php artisan view:cache",
  "outputDirectory": "public",
  "build": {
    "env": {
      "PHP_VERSION": "8.2"
    }
  },
  "routes": [
    {
      "src": "/build/assets/(.*)",
      "dest": "/build/assets/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.*\\.php)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}